    <webLinks>
        <fullName>Send_Notification_Mail</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send Notification Mail</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)} 


if(&apos;{!Account.ParentId}&apos; == null || &apos;{!Account.ParentId}&apos; == &apos;&apos;) 
{ 
alert(&quot;Error - Parent Account is not populated&quot;); 
} 
else 
{ 
var result= sforce.connection.query(&quot;select Id,Type, Primary_Email__c from account where id= &apos;{!Account.ParentId}&apos; &quot;); 
var record= result.getArray(&quot;records&quot;); 

if(record[0].Type != &apos;Distributor&apos;) 
{ 
alert(&quot;Error - Parent Account is not of type Distibutor&quot;); 
} 
else if(record[0].Primary_Email__c == null) 
{ 
alert(&quot;Error - Parent Account has no Primary Email&quot;); 
} 
else 
{ 
var ParentName = sforce.apex.execute(&quot;NotifyOpportunity&quot;,&quot;SendNotificationMail&quot;, {&apos;oppId&apos;: &apos;{!Opportunity.Id}&apos; }); 
alert(&quot;An email has been sent to the Distributor (Parent Account)&quot;) 
} 
}</url>
