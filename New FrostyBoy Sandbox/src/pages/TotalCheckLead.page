                    <apex:pageBlockSectionItem id="suburbItem">
                        <apex:outputLabel value="City" for="suburb" />
                        <apex:outputPanel >
                            <apex:inputText value="{!suburb}" id="suburb"
                                onkeypress="return noenter(event)" />
                            <apex:outputPanel id="suburbSuggestWrap" layout="block"
