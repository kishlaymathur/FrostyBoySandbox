        <fullName>Ownership_cannot_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  Owner.Id  ==  $User.Id ,$Profile.Name  &lt;&gt; &apos;System Administrator&apos;  )</errorConditionFormula>
        <errorMessage>You cannot change the details if you are not an admin or the owner.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Parent_Account_Not_same_as_account_name</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISNEW() ),Parent.Id =  Id)</errorConditionFormula>
        <errorDisplayField>ParentId</errorDisplayField>
        <errorMessage>Account can&apos;t be it&apos;s parent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Store_Mandatory_if_Classification_Chain</fullName>
